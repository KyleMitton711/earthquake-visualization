{
  "name": "earthquake-visulization",
  "version": "1.0.0",
  "description": "A real-time earthquake visualization of USGS Earthquake Hazards Program using Nuxt",
  "author": "Robert Soriano",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
